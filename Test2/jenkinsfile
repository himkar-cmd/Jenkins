pipeline {
    agent any
    tools {
        maven 'maven'
    }
    stages ('checkout') {
        steps { branch:'main',url:'https://github.com/himkar-cmd/Jenkins.git'}
    }
    stage ('Build'){
        steps {sh 'mvn clean package'}
    }
    stage ('test') {
        steps {echo 'Hello'}
    }
    stage ('deploy') {
        steps {echo 'deploy'}
    }
}
